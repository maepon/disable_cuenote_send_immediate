{
  "manifest_version": 2,
  "name": "Disable Cuenote Send Immediate",
  "version": "1.0.0",
  "description": "ユミルリンク「Cuenote FC」の即時配信の選択を無効化する機能拡張",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "icon19.png"
    }
  },
  "background": {
    "scripts": ["scripts/background.js"]
  },
  "permissions":[
    "activeTab",
    "https://*.asp.cuenote.jp/*"
  ]
}

